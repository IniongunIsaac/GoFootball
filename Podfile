  pod 'RxCocoa', '~> 5'
  pod 'RxSwiftExt', '~> 5'
  pod 'RxAlamofire', '~> 5'
  pod 'RxDataSources', '~> 4.0'
  
  #Other pods
  pod 'R.swift'
  pod 'DeviceKit', '~> 4.0'
  pod 'HorizontalProgressBar'
  pod 'Kingfisher'
  pod 'Toast-Swift', '~> 5.0.1'
  pod 'BottomPopup'
  pod 'PocketSVG', '~> 2.0'

  target 'GoFootballTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
  end

  target 'GoFootballUITests' do
    # Pods for testing
  end

end
